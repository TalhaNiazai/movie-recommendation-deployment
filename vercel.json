{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=16384"
  },
  "functions": {
    "app.py": {
      "maxDuration": 10
    }
  }
}
